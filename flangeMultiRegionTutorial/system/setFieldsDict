FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue DT 4e-05
);

regions
(
    boxToCell
    {
        box (0 -0.1 -0.1) (0.05 0.1 0.1);
        fieldValues
        (
            volScalarFieldValue DT 4e-06
        );
    }
);


